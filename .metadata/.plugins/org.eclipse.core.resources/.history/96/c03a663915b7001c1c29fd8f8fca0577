package com.spring.diex8;

import org.springframework.context.ApplicationContext;
import org.springframework.context.annotation.AnnotationConfigApplicationContext;

public class Program {

	public static void main(String[] args) {
	
//		ApplicationContext  context =
//				new ClassPathXmlApplicationContext("AnnoXmlSetting.xml");
//		
		ApplicationContext  context =
				new AnnotationConfigApplicationContext("setting_ano.xml");
		
		Exam exam = (Exam) context.getBean("exam2");
		System.out.println(exam.toString());
		
		ExamConsole eCon = context.getBean(ExamConsole.class);
		eCon.print();


	}

}
